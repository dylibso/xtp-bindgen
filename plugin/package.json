  "license": "BSD-3-Clause",
  "devDependencies": {
    "@extism/js-pdk": "^1.0.1",
    "@types/js-yaml": "^4.0.9",
    "esbuild": "^0.19.6",
    "prettier": "^3.3.2",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@dylibso/xtp-bindgen": "file://../",
    "ajv": "^8.17.1",
    "js-yaml": "^4.1.0"
  },
